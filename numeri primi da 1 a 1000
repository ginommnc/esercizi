
#include <stdio.h>

int prime (int n){
  for (int i=2; i<n; i++){
    if (n%i==0){
    return 0;
    }
  
  }  
      return 1;
}

int main (){

  int f=0;
  
  for (int c=1; c<=1000;++c){
  
 
  
    
    if (prime(c)){
      ++f;
      printf("%6d",c);  
      
      
      if (f%10==0){
        puts("\n");
      }
      
      
    }
  }
}


