#include<stdio.h>
#include<stdlib.h>
#include<time.h>

#define N 3
#define M 3



int main (void){
  
  int a[N][M]={1,2,3,4,5,6,7,8,9};//matrice a
  int b[N][M]={1,2,3,4,5,6,7,8,9};//matrice b
  int differenza[N][M];//matrice differenza

  size_t row,col;
  
    //a-b=differenza
    for(row=0;row<N;++row){
      for(col=0;col<M;++col){
      
      differenza[row][col]=a[row][col]-b[row][col];
      printf("%d\t",differenza[row][col]);


    }

puts("\n");
}
}
