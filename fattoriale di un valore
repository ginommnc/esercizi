#include <stdio.h>

unsigned long long factorial(unsigned int a){
  if (a<0){
    return a;
  }
    else if (a<=1){
      return 1;
    }
    else if (a>1){
      return (a*factorial(a-1));
    }
 }

int main (void){
  int counter;
  scanf ("%d", &counter);
  
    printf("%u!=%llu\n",counter,factorial(counter));
  
}

